<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="theme_dialog_alert">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <style name="title">
        <item name="android:textSize">17.0sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:shadowColor">#ff000000</item>
    </style>
    <style name="MyGallery">
        <item name="android:gravity">center_vertical</item>
        <item name="android:fadingEdge">none</item>
        <item name="android:spacing">-20.0dip</item>
        <item name="android:unselectedAlpha">0.85</item>
    </style>
    <style name="back">
        <item name="android:textSize">13.0sp</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="enter">
        <item name="android:textSize">13.0sp</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="roomRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:maxHeight">37.0dip</item>
        <item name="android:progressDrawable">@layout/room_rating_bar</item>
        <item name="android:minHeight">20.0dip</item>
    </style>
    <style name="Theme" parent="@android:style/Theme">
        <item name="android:windowAnimationStyle">@style/Animation</item>
    </style>
    <style name="Animation">
        <item name="android:activityOpenEnterAnimation">@null</item>
        <item name="android:activityOpenExitAnimation">@null</item>
        <item name="android:activityCloseEnterAnimation">@null</item>
        <item name="android:activityCloseExitAnimation">@null</item>
        <item name="android:taskOpenEnterAnimation">@null</item>
        <item name="android:taskOpenExitAnimation">@null</item>
        <item name="android:taskCloseEnterAnimation">@null</item>
        <item name="android:taskCloseExitAnimation">@null</item>
        <item name="android:taskToFrontEnterAnimation">@null</item>
        <item name="android:taskToFrontExitAnimation">@null</item>
        <item name="android:taskToBackEnterAnimation">@null</item>
        <item name="android:taskToBackExitAnimation">@null</item>
    </style>
</resources>
